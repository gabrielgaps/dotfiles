!! URxvt Appearance
!URxvt.font: xft:Inconsolata:size=10
!URxvt.font: xft:Mono:pixelsize=14:antialias=true
URxvt.font: xft:lucy tewi:pixelsize=13,xft:Cozette:pixelsize=13
!URxvt.boldFont: xft:
!URxvt.italicFont: xft:DejaVu Sans Mono:pixelsize=15,style=Book
!URxvt.boldItalicfont: xft:DejaVu Sans Mono:pixelsize=15,style=Book
URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.geometry: 92x24
!URxvt.internalBorder: 24
URxvt.internalBorder: 4
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 2048
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: true
URxvt.depth: 16
URxvt.iso14755: false
!URxvt.background: [85]#282828

!URxvt.transparent:   true
!URxvt.shading:       40

!! URxvt Functions
URxvt.perl-ext-common: font-size,selection-to-clipboard,pasta,resize-font
URxvt.perl-lib: /usr/bin/perl
URxvt.clipboard.autocopy: true
URxvt.keysym.C-j:       perl:font-size:increase
URxvt.keysym.C-k:       perl:font-size:decrease
